/* Flexslider */

.flexslider  ul li{
background-image:none;
padding:0px;
margin: 0px;
}

.flexslider{
margin-top:0px;
margin-bottom:0px;
padding:3px;
background:#222;
border:1px solid #333;
box-shadow:none;
}

.flexslider .flex-control-nav{
bottom:0px;
}

.flex-control-paging li a{
background:#fff;
box-shadow:0px 0px 3px #555;
border-radius: 10px !important;
}

.flex-control-paging li a:hover{
background:#ccc;
}

.flex-control-paging li a.flex-active{
background:#ccc;
}

.flexslider .slides > li {
position:relative;
}

.flex-direction-nav .flex-next {right: 0px; }
.flex-direction-nav .flex-prev {left: 0px;}

.flex-caption{
	width: 50%;
	padding-top: 0px;
	padding-left: 0px;
	margin: 0 0 120px 60px;
	background-color: #222;
	background: rgba(0, 0, 0, 0.9);
	position: absolute;
	display: block;
	left: 0;
	bottom: 0;
}

.flex-caption h3 {
	color: #fff;
	font-size: 23px;
	line-height: 33px;
	padding-left: 20px;
	padding-right: 20px;
	padding-top: 20px;
	padding-bottom: 0px;
	margin:0;
   	margin-bottom:10px;
	line-height: 22px;
	font-weight: normal;
}
	
.flex-caption p {
	padding-left: 20px;
	padding-bottom: 20px;
	padding-right: 20px;
	margin:0;
	font-size: 14px;
	line-height: 21px;
	font-style: italic;
	color: #fff;
	font-weight: normal;
}

